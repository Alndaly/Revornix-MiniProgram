{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"pages": [
		{
			"path": "pages/document/index",
			"style": {
				"navigationBarTitleText": "文档",
				"enablePullDownRefresh": true,
				"onReachBottomDistance": 100
			}
		},
		{
			"path": "pages/document/create/index",
			"style": {
				"navigationBarTitleText": "创建文档",
				"enablePullDownRefresh": true,
				"onReachBottomDistance": 100
			}
		},
		{
			"path": "pages/section/index",
			"style": {
				"navigationBarTitleText": "专栏",
				"enablePullDownRefresh": true,
				"onReachBottomDistance": 100
			}
		},
		{
			"path": "pages/mine/index",
			"style": {
				"navigationBarTitleText": "我的",
				"enablePullDownRefresh": true,
				"onReachBottomDistance": 100
			}
		},
		{
			"path": "pages/document/detail/index",
			"style": {
				"navigationBarTitleText": "文档详情",
				"enablePullDownRefresh": true,
				"onReachBottomDistance": 100
			}
		},
		{
			"path": "pages/section/detail/index",
			"style": {
				"navigationBarTitleText": "专栏详情",
				"enablePullDownRefresh": true,
				"onReachBottomDistance": 100
			}
		},
		{
			"path": "pages/test/index",
			"style": {
				"navigationBarTitleText": "测试",
				"enablePullDownRefresh": true,
				"onReachBottomDistance": 100
			}
		}
	],
	"subpackages": [],
	"tabBar": {
		"custom": true,
		"list": [
			{
				"pagePath": "pages/document/index"
			},
			{
				"pagePath": "pages/section/index"
			},
			{
				"pagePath": "pages/mine/index"
			}
		]
	},
	"globalStyle": {
		"navigationBarTitleText": "Revornix",
		"usingComponents": {
			"towxml": "/wxcomponents/towxml/towxml"
		}
	}
}